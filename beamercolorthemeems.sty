\definecolor{emsgray}{RGB}{231, 231, 231}
\definecolor{emslightgray}{RGB}{217, 217, 217}
\definecolor{darkblue}{RGB}{39, 60, 117}

% ====================
% Theme
% ====================

% Basic colors
\setbeamercolor{palette primary}{fg=black, bg=white}
\setbeamercolor{palette secondary}{fg=black, bg=emsgreen}
\setbeamercolor{palette tertiary}{fg=black, bg=emsgray}
\setbeamercolor{palette quaternary}{fg=black, bg=white}
\setbeamercolor{structure}{fg=darkblue}
% Headline
\setbeamercolor{headline}{fg=black,bg=white}
\setbeamercolor{headline rule}{bg=black}


% Block
\setbeamercolor{block title}{fg=black,bg=white}
\setbeamercolor{block separator}{bg=black}
